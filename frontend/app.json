{
  "expo": {
    "name": "buzzbitebeta",
    "slug": "buzzbitebeta",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BuzzBite to access your camera",
          "microphonePermission": "Allow BuzzBite to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.buzzbitebeta",
      "infoPlist": {
        "NSCameraUsageDescription": "Your app needs access to the camera to take photos/videos.",
        "NSPhotoLibraryUsageDescription": "Your app needs access to the photo library to select photos/videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.buzzbitebeta",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ab7f8ec-b5ae-4ce5-aed5-78e1d7146395"
      }
    },
    "owner": "tsong28"
  }
}
